{
    "name": "aleex1848/blade-hwkicons",
    "version": "1.0.3",
    "description": "A package to easily make use of Hwkicons in your Laravel Blade views.",
    "keywords": ["Blade", "Hwkicons", "Laravel"],
    "homepage": "https://www.hwk-do.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Dieckmann"
        }
    ],
    "require": {
        "php": "~8.1.0|~8.2.0",
        "blade-ui-kit/blade-icons": "^1.4",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Aleex1848\\BladeHwkicons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aleex1848\\BladeHwkicons\\BladeHwkiconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
